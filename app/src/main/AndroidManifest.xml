<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.organicasti.localasti">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/localastinew"
        android:label="LocalAsti"
        android:roundIcon="@mipmap/localastinew_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".customer.FarmerProfileForCustomer"></activity>
        <activity
            android:name=".Admin"
            android:label="ADMIN"></activity>
        <activity
            android:name=".customer.Delivery"
            android:label="Delivery" />
        <activity
            android:name=".vendor.Vendor_Activity"
            android:label="MilkAsti"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".customer.Customer_Act"
            android:label="MilkAsti"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".UserDetails" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <service
            android:name=".service.MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT"/>
            </intent-filter>
        </service>
    </application>

</manifest>